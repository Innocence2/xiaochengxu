page{
  box-sizing: border-box;
}
.slide-image{
  width:100%;
  height:300rpx;
}
.index-nav{
  display: flex;
  flex-direction: wrap;
}
.index-nav .nav-item{
  width:33.3333%;
  height: 200rpx;
  border:solid 1px #fcc;
  display: flex;
  flex-direction: nowrap;
  justify-content: center;
  align-items: center;
  flex-flow: column;
  font-size: 14px;
  color:#fcc;
  
}
.nav-item image{
  width: 80rpx;
  height: 80rpx;
}
/* 分区入口 */
.index-enter{
  display: flex;
 padding:20rpx 30rpx;
}
.index-enter .item{
 width:50%;
 display: flex;
 justify-content: center;
}
.index-enter image{
   width: 320rpx;
   height: 178rpx;
}
